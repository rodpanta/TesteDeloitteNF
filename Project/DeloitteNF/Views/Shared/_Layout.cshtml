                    </a>
                    <ul class="sub">
                        <li>@Html.ActionLink("Vendas Por Produto", "ProductsSales", "Relatorios")</li>
                        <li>@Html.ActionLink("Vendas Por Cliente", "ClientsSales", "Relatorios")</li>
                    </ul>
                </li>


                </ul>
                <!-- sidebar menu end-->
