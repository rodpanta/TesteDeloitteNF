@model DeloitteNF.Models.stpProductsId_Result

@{
    ViewBag.Title = "Details";
}

<div class="form-horizontal">
    <!-- INLINE FORM ELELEMNTS -->
    <div class="row mt">
        <div class="col-lg-12">
            <div class="form-panel">
                <h4 class="mb"><i class="fa fa-angle-right"></i>@Html.DisplayFor(model => model.nomeprod)</h4>
                <form class="form-inline" role="form">

                    <div>
                        <h4></h4>
                        <hr />
                        <dl class="dl-horizontal">
                            <dt>
                                Código Produto
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Produto_id)
                            </dd>

                            <dt>
                                Produto
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.nomeprod)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.Unidade)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Unidade)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.Preco)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Preco)
                            </dd>

                            <dt>
                                Código Fornecedor
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.Fornecedor_id)
                            </dd>

                            <dt>
                                Fornecedor
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.nomefornec)
                            </dd>

                        </dl>
                    </div>
                </form>
            </div>
            <!-- /form-panel -->
        </div>
        <!-- /col-lg-12 -->
    </div>
    <!-- /row -->
    <!-- INPUT MESSAGES -->


</div>


